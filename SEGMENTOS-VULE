# Primero elimine todas las entradas antiguas
:foreach i in=[/ip firewall address-list find ] do={
  :if ( [/ip firewall address-list get $i comment] = "SEGMENTOS-VULE") do={
      /ip firewall address-list remove $i
  }
}

